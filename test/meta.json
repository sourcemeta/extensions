{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [ "title", "description", "examples" ],
  "properties": {
    "$id": false,
    "$schema": {
      "const": "https://json-schema.org/draft/2020-12/schema"
    },
    "$vocabulary": {
      "type": "object",
      "minProperties": 1
    },
    "$dynamicAnchor": {
      "const": "meta"
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": "string",
      "minLength": 1
    },
    "examples": {
      "type": "array",
      "minItems": 3,
      "uniqueItems": true
    }
  }
}
